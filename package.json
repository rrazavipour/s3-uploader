{
  "name": "IMEI-interrogation-utility",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest --verbose",
    "build": "tsc",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/server.js",
    "deploy": "npm run build && npm run start",
    "watch-node": "ENV=dev nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    }
  },
  "author": "Reza Razavipour",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/uuid": "^3.4.6",
    "cross-conf-env": "^1.1.2",
    "install": "^0.12.2",
    "jest": "^24.9.0",
    "npm": ">=6.13.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "aws-sdk": "^2.631.0",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  }
}
